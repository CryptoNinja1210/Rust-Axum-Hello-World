{
    "version": 2,
    "builds": [
      {
        "src": "src/main.rs",
        "use": "vercel-rust@4.0.6",
        "config": { "runtime": "vercel-rust@4.0.6" }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "ui/staticfiles"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "src/main.rs"
      }
    ],
    "outputDirectory": "ui/staticfiles"
  }